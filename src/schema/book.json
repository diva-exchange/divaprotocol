{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "/schema/book.json",

  "type": "object",
  "properties": {
    "contract": {
      "type": "string",
      "pattern": "^[A-Z0-9]{2,6}_[A-Z0-9]{2,6}$"
    },
    "buy": {
      "$ref": "recordBuySell.json"
    },
    "sell": {
      "$ref": "recordBuySell.json"
    }
  },
  "additionalProperties": false,
  "required": [
    "contract",
    "buy",
    "sell"
  ]
}
